extends ../template

include ../mixins/goods-slider

block variables
  - var title = 'Главная'

block content
  section.banner 
    picture
      source(type="image/webp" srcset="images/banner.jpg")
      img.banner__img(src="images/banner.jpg" alt="Эргономичные матрасы и изделия для комфортного сна")
    .container.banner__container
      h1.banner__title.h1title Эргономичные матрасы и изделия для комфортного сна
      p.banner__text Производство фабрики «Торбеевские матрасы»
      .banner__btns 
        a.banner__btn.banner__btn--background(href="#") Выбрать матрас
        button.banner__btn.banner__btn--border Проконсультироваться

  section.cards 
    .container
      h2.visually-hidden Акции
      ul.cards__actions.cards-actions
        li.cards-actions__item
          a.cards-actions__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/action1.jpg")
              img.cards-actions__img(src="images/action1.jpg" alt="Бесплатно утилизируем матрас")
            .cards-actions__info
              h3.cards-actions__title Бесплатно утилизируем матрас
        li.cards-actions__item
          a.cards-actions__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/action2.jpg")
              img.cards-actions__img(src="images/action2.jpg" alt="-10% на вторую покупку")
            .cards-actions__info
              h3.cards-actions__title -10% на вторую покупку
              p.cards-actions__text Кроме подушки
        li.cards-actions__item
          a.cards-actions__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/action3.jpg")
              img.cards-actions__img(src="images/action3.jpg" alt="Подарки от 10 000 руб.")
            .cards-actions__info
              h3.cards-actions__title Подарки от 10 000 руб.

  section.cards 
    .container
      h2.visually-hidden Категории
      ul.cards__categories.cards-categories
        li.cards-categories__item
          a.cards-categories__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/category1.jpg")
              img.cards-categories__img(src="images/category1.jpg" alt="Матрасы")
            h4.cards-categories__title.arrow-after Матрасы
        li.cards-categories__item
          a.cards-categories__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/category2.jpg")
              img.cards-categories__img(src="images/category2.jpg" alt="Наматрасники")
            h4.cards-categories__title.arrow-after Наматрасники
        li.cards-categories__item
          a.cards-categories__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/category3.jpg")
              img.cards-categories__img(src="images/category3.jpg" alt="Подушки")
            h4.cards-categories__title.arrow-after Подушки
        li.cards-categories__item
          a.cards-categories__link(href="#") 
            picture 
              source(type="image/webp" srcset="images/category4.jpg")
              img.cards-categories__img(src="images/category4.jpg" alt="Кроватные основания")
            h4.cards-categories__title.arrow-after Кроватные основания

